<% content_for :footer do %>
    <%= javascript_include_tag("time-series.js", "widgets/timeseries", "modules/inside-government/weekly-visitors") %>
<% end %>

<article id="weekly-visitors-module" class="dashboard one-up">
    <%= graph_image_tag "government/visitors/weekly" %>

    <div class="datainsight-hidden">
      <hgroup class="module-header left">
        <h1 class="graph-info">Weekly unique visitors</h1>

        <p class="graph-heading">Unique visitors per week to <span class="insidegov">Inside Government</span></p>
      </hgroup>
      <figure id="inside-gov-weekly-visitors" class="graph-container"></figure>
      <p class="graph-source">Source: Google Analytics</p>
    </div>

    <%= render partial: "inside_government/modules/inside_gov_annotations" if Settings.feature_toggles[:inside_government_annotations] %>
</article>
